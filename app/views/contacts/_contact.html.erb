<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= contact.street %>
  </p>

  <p>
    <strong>City:</strong>
    <%= contact.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= contact.state %>
  </p>

  <p>
    <strong>Zip:</strong>
    <%= contact.zip %>
  </p>

</div>
